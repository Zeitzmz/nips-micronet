net: "sparsity/ProfitableNet.prototxt"
test_iter: 2000
test_interval: 5000
test_initialization: true
display:  20
average_loss: 20

dsd_phase: 0 ## 0 means sparse finetune
dsd_mask_file: "sparsity/mask.bin"

base_lr: 0.001
gamma: 0.1
lr_policy: "step"
stepsize: 200000
max_iter: 400000

momentum: 0.9
weight_decay: 0.00004
snapshot: 50000
snapshot_prefix: "dsd/snapshots/caffe_train_sparsity"
solver_mode: GPU
